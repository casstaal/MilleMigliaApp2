generator client {
    provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
      id            String         @id @default(uuid())
      name          String
      email         String         @unique
      password_hash String
      role          String
}

model Check {
      id            String         @id @default(uuid())
      title         String
      imgUrl        String
      latitude      Float
      longitude     Float
      date          DateTime
}